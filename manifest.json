{
  "manifest_version": 2,

  "name": "duckduckgo-to-google",
  "description": "Do a Google search directly from DuckDuckGo",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}